{
  "name": "spec-driven-development-starter",
  "version": "1.0.0",
  "description": "Spec-Driven Development template with GitHub Copilot integration",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "update-quick-ref": "node tools/update-quick-reference.js",
    "update-copilot-instructions": "node tools/update-copilot-instructions.js",
    "update-todos": "node tools/update-todos.js",
    "update-spaces-mapping": "node tools/update-spaces-mapping.js",
    "update-context": "node tools/update-all-context.js",
    "extract-specs": "node tools/spec-extractor.js",
    "db:migrate": "node database/migrate.js",
    "db:seed": "node database/seed.js",
    "db:setup": "node database/setup.js",
    "db:reset": "npm run db:setup"
  },
  "keywords": [
    "spec-driven",
    "development",
    "github-copilot",
    "template",
    "vscode"
  ],
  "author": "Michael McCullough",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.1",
    "helmet": "^8.1.0",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "pg": "^8.16.3",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.13",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "supertest": "^7.0.0"
  }
}
